html{
  	-moz-osx-font-smoothing: grayscale;
	-webkit-font-smoothing: antialiased;
}
body{
  margin:0px;
  background: #0C0C16;
}
div, div:focus, div:active{
  outline:none;
}
main, main:focus, main:active{
  outline:none;
}
body, body:focus, body:active{
  outline:none;
}
*, *:focus, *:active{
  outline:none;
}

.shippingBar{
  font-size: 14px;
  font-family: "PTRegular", sans-serif;
  text-transform: uppercase;
  letter-spacing: 1.17px;
  text-align: center;
  color: white;
  background: #0C0C16;
  padding: 9px 0px;
  span{
    color: #A6CE39;
    padding: 0px 4px;
  }
}
.scrolled{
  top:0px !important;
}
.scrolled.popupCart{
  transform:translateX(-50%) translateY(-16px) !important;
}
.navBar.scrolled{
  background:#0C0C16;
  height:75px !important;
  .inner{
    a img{
      height:80% !important;
      top:10%;
    }
    .controls{
      margin-top:25px !important;
    }
  }
}

.navBar{
  height:107px;
  transition:all .2s;
  position:fixed;
  top:37px;
  background:transparent;
  width:100%;
  z-index:20;
  .inner{
    max-width:1280px;
    width:90%;
    margin:auto;
    height:100%;
    display:flex;
    justify-content:space-between;
    .logo{
      width:600px;
      height:100%;
      cursor:pointer;
      img{
        width:100%;
        height:100%;
        display:block;
        object-fit:contain;
        object-position:left;
        position:relative;
        transition:all .2s;
        top:0%;
      }
    }
    .controls{
      display:flex;
      margin-top:37px;
      transition:all .2s;
      .locator{
        height:32px;
        margin-right:60px;
        cursor:pointer;
        img{
          height:100%;
          width:auto;
          position:relative;
          top:-2px;
        }
      }
      .cartIcon{
        height:28px;
        margin-right:60px;
        cursor:pointer;
        position:relative;
        z-index:5;
        img{
          height:100%;
          width:auto;
        }
      }
      .burgerIcon{
        width:56px;
        height:56px;
        cursor:pointer;
        display:flex;
        flex-direction:column;
        justify-content:center;
        position:relative;
        top:-15px;
        overflow:hidden;
        img{
          height:4px;
          display:block;
          width:auto;
          margin:4px 0px;
          
        }
        .shortX{
          position:absolute;
          left:-70px;
        }
        .longX{
          position:absolute;
          left:70px;
        }
      }
    }
  }
}
.popupCart{
  position:fixed;
  max-width:1280px;
  width:90%;
  //transition:all .2s;
  left:50%;
  transform:translateX(-50%);
  top:37px;
  z-index:100;
  .inner{
    max-width:400px;
    margin-left:auto;
    margin-right:76px;
    background-image:url(https://cdn.shopify.com/s/files/1/0046/8326/8165/files/dark_wall_f008e3c8-2a45-4a98-99a4-73459fa97eb0.png?1653935293538183576);
    background-repeat:repeat;
    padding-bottom:40px;
    .cartIcon{
      height:28px;
      margin-right:60px;
      cursor:pointer;
      position: absolute;
      top: 41px;
      right: 56px;
      img{
        height:100%;
        width:auto;
      }
    }
    .title{
      color:white;
      font-size:25px;
      font-family:"YKRegular", sans-serif;
      letter-spacing:2.5px;
      text-transform:uppercase;
      padding:40px;
      padding-bottom:26px;
    }
    .itemCont{
      width:80%;
      margin:auto;
      border-top:2px solid #0C0C16;
      border-bottom:2px solid #0C0C16;
      .empty{
        img{
          width:100%;
          height:267px;
          object-fit:contain;
        }
        span{
          display:block;
          color:white;
          text-align:center;
          font-size:16px;
          font-family:"Tele", sans-serif;
          padding-top:20px;
          padding-bottom:22px;
        }
      }
      .prodRow{
        height:60px;
        width:100%;
        padding-top:30px;
        display:flex;
        justify-content:space-between;
        img{
          width:60px;
          height:100%;
          object-fit:contain;
        }
        .text{
          color:white;
          display:flex;
          flex-direction:column;
          font-size:15px;
          font-family:"PTRegular", sans-serif;
          width:210px;
          .prodTitle{
            padding-bottom:10px;
            text-transform:uppercase;
            letter-spacing:3px;
          }
          .price{
            letter-spacing:.11px;
          }
        }
        .exit{
          cursor:pointer;
          width:20px;
          height:20px;
          background:#929292;
          border-radius:100%;
          text-align:center;
          line-height:19px;
        }
        &:last-of-type{
          padding-bottom:30px;
        }
      }
    }
    .subRow{
      color:white;
      letter-spacing:3px;
      font-size:15px;
      text-transform:uppercase;
      font-family:"PTRegular", sans-serif;
      padding:40px;
      text-align:right;
      padding-top:30px;
      padding-bottom:16px;
      span{
        font-size:16px;
        letter-spacing:.11px;
      }
    }
    .checkout{
      background-image:url(https://cdn.shopify.com/s/files/1/0046/8326/8165/files/checkout.png?6010977967374065174);
      background-size:cover;
      background-position:left;
      height:32px;
      width:80%;
      margin:auto;
      font-family:"PTBold", sans-serif;
      font-size:12px;
      letter-spacing:2.4px;
      text-transform:uppercase;
      text-align:center;
      padding-top:18px;
      cursor:pointer;
      text-decoration:none;
      display:block;
      color:#0c0c16;
    }
  }
}
.dropNav{
  position:fixed;
  top:37px;
  width:100%;
  z-index:10;
  .background{
    position: absolute;
    background-image: url(https://cdn.shopify.com/s/files/1/0046/8326/8165/files/dark_wall_f008e3c8-2a45-4a98-99a4-73459fa97eb0.png?1653935293538183576);
    width: 120%;
    left: -10%;
    transform: rotate(5.75deg);
    height: 580px;
    top: -180px;
    border-bottom: 2px solid #BFD730;
    overflow: hidden;
    img{
      transform: rotate(-5.75deg) translateX(-50%);
      height: 71%;
      position: absolute;
      top: 60px;
      left: calc(50% + 80px);
      width: 1440px;
      object-fit: contain;
      object-position: left;
    }
  }
  .navRow{
    display: flex;
    position: absolute;
    width: 90%;
    max-width:1440px;
    left: 50%;
    transform: translateX(calc(-50% + 47px));
    justify-content: flex-end;
    top: 112px;
    a{
      text-decoration: none;
      margin-right: 57px;
      cursor:pointer;
      transition:all .2s;
      &:hover{
        icon{
          color:#929292;
        }
        span{
          color:#929292;
        }
      }
      icon{
        display: block;
        height: 48px;
        color: #BFD730;
        padding-bottom: 16px;
        transition:all .2s;
        svg{
          height: 48px;
          margin: auto;
          display: block;
        }
      }
      span{
        color: white;
        font-size: 25px;
        font-family: "YKRegular", sans-serif;
        text-transform: uppercase;
        letter-spacing: 2.5px;
        text-align: center;
        transition:all .2s;
      }
    }
  }
  .contactInfo{
    width: 90%;
    max-width: 1440px;
    display: flex;
    left: 50%;
    flex-direction: column;
    position: absolute;
    transform: translateX(-50%);
    justify-content: flex-end;
    top: 298px;
    .phone{
      text-align:right;
      a{
        font-size: 16px;
        color: white;
        font-family: "PTRegular", sans-serif;
        text-decoration: none;
      }
    }
    .email{
      text-align:right;
      margin-top:10px;
      a{
        font-size: 16px;
        color: white;
        font-family: "PTRegular", sans-serif;
        text-decoration: none;
      }
    }
    .socialMediaFlex{
      display:flex;
      justify-content:flex-end;
      height:40px;
      a{
        img{
          transform: scale(.5);
          height: 100%;
          object-fit: contain;
          width: 42px;
          margin-right: -10px;
          cursor:pointer;
        }
      }
    }
  }
}
.loadingCover{
  display: block;
  position: fixed;
  width: 100vw;
  top: 0px;
  left: 0px;
  z-index: 100;
  background-image: url(https://cdn.shopify.com/s/files/1/0046/8326/8165/files/dark_wall_f008e3c8-2a45-4a98-99a4-73459fa97eb0.png?1653935293538183576);
  height: 100vh;
  .inner{
    color: #92C840;
    width: 65%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%) rotate(-13deg);
  }
}
.noscroll{
  overflow:hidden;
}
.homeHeroContainer{
  overflow:hidden;
  height:720px;
  position:relative;
  .videoContainer{
    width:100%;
    height:100%;
    video{
      width:100%;
      height:100%;
      object-fit:cover;
    }
    img{
      width:100%;
      height:100%;
      object-fit:cover;
    }
  }
  .text{
    max-width: 1280px;
    width: 90%;
    position: absolute;
    bottom: 180px;
    left: 50%;
    transform: translateX(-50%) translateY(100%);
    .line{
      background: #BFD730;
      width: 80px;
      height: 2px;
    }
    .superTitle{
      font-size: 30px;
      font-family: "YKRegular", sans-serif;
      color: white;
      text-transform: uppercase;
      letter-spacing: 3px;
      padding-top: 15px;
    }
    .title{
      font-size: 40px;
      font-family: "Tele", sans-serif;
      color: white;
      padding-top: 7px;
    }
  }
}
.stickerContaier{
  width:100%;
  max-width:1440px;
  position: absolute;
  left:50%;
  transform:translateX(-50%);
  top: 610px;
  .sticker.unload{
    transform: scale(5) rotate(-20deg);
    filter: blur(8px);
    opacity:0;
  }
  .sticker{
    position: absolute;
    display: flex;
    flex-direction: column;
    width: 323px;
    height: 151px;
    border: 6px solid white;
    right: 48px;
    border-radius: 9px;
    transform: scale(1) rotate(-4deg);
    filter: blur(0px);
    opacity:1;
    transition:all .5s cubic-bezier(1, 0.01, 0.81, 1.15);
    .top{
      border-bottom: 6px solid white;
      height: 53px;
      span{
        font-family: "Tele", sans-serif;
        color: white;
        font-size: 37px;
        margin-left: 15px;
        position: relative;
        letter-spacing: 8px;
        top: 10px;
      }
      img{
        display: block;
        width: 37px;
        float: right;
        padding-right: 22px;
        padding-top: 11px;
        padding-bottom: 9px;
      }
    }
    .bottom{
      display:flex;
      .left{
        img{
          width: 126px;
          height: 133%;
          margin-left: -18px;
          margin-top: -19px;
          object-fit: contain;
          position: relative;
          z-index: 2;
        }
      }
      .right{
        margin-left: -22px;
        border-left: 6px solid white;
        color: white;
        font-family: PTRegular;
        font-size: 10px;
        padding: 15px;
        line-height: 15px;
      }
    }
  }
  @media only screen and (max-width: 1066px){
    .sticker{
      transform: scale(.8) rotate(-4deg);
      right:-12px;
    }
  }
}
