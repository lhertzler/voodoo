        .prod7{
          position: absolute;
          transform: scale(.35) rotate(45deg) translate(641px, 1500px);
          transition:all .2s;
          transition-delay:.7s;
        }
        .prod7.shoot{
          transform: scale(.35) rotate(45deg) translate(641px, 565px);
        }
        .prod8{
          position: absolute;
          transform: scale(.35) rotate(45deg) translate(1190px, 1800px);
          transition:all .2s;
          transition-delay:.8s;
        }
        .prod8.shoot{
          transform: scale(.35) rotate(45deg) translate(1190px, 564px);
        }
        .prod9{
          position: absolute;
          transform: scale(.35) rotate(45deg) translate(855px, 1300px);
          transition:all .2s;
          transition-delay:.9s;
        }
        .prod9.shoot{
          transform: scale(.35) rotate(45deg) translate(855px, 336px);
        }
      }
      .textBlock{
        
        .arrow{
          top:-140px;
          right:-323px;
          transform:scale(0.34);
        }
