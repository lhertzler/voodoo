        .prod2{
          position: absolute;
          transform: scale(.35) rotate(45deg) translate(10px, 750px);
          transition:all .2s;
          transition-delay:.2s;
        }
        .prod2.shoot{
          transform: scale(.35) rotate(45deg) translate(10px, 40px);
