        .prod3{
          position: absolute;
          transform: scale(.35) rotate(45deg) translate(70px, 950px);
          transition:all .2s;
          transition-delay:.3s;
        }
        .prod3.shoot{
          transform: scale(.35) rotate(45deg) translate(70px, 344px);
        }
        .prod4{
          position: absolute;
          transform: scale(.35) rotate(45deg) translate(215px, 1060px);
          transition:all .2s;
          transition-delay:.4s;
